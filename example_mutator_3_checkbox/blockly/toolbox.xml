<category id="catDeviceStatus"> 
  <block type="boardevent">
    <value name="device">
      <block type="text">
        <field name="TEXT"></field>
      </block>
    </value>    
  </block>  
  <block type="boardevent_sendmessage">
    <value name="cmd">
      <block type="text">
        <field name="TEXT">0xF0, 0x0E, 0x07, 0xF7</field>
      </block>
    </value> 
  </block>     
  <block type="boardevent_getmessage">
  </block>    
 </category>
